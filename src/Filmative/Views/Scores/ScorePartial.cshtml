<div id="display-score-partial">
    <h2>Attemping to Submit a Score</h2>
    <form action="NewScore" class="new-score">
        <label for="selectTitle">Title: </label>
        <input type="number" name="selectTitle" />
        <label for="newRating">Rating: </label>
        <input type="number" name="newRating" />
        <label for="newReview">Review: </label>
        <input type="text" name="newReview" />
        <button type="submit">Submit</button>
    </form>
    <div id="submitResult"></div>
</div>    